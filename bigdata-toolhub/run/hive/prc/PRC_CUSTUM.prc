v_sql := v_sql||'                                        ELSE ';
v_sql := v_sql||'                                           todate(S.START_DATE,''yyyy-mm-dd'') ';
v_sql := v_sql||'                                     END ';
v_sql := v_sql||'                                     ) days, ';
v_sql := v_sql||'                                     COAL (S.STM_BALFRE, 0) ';
v_sql := v_sql||'                                   + ';
v_sql := v_sql||'                                    CASE ';
v_sql := v_sql||'                                        WHEN S.STMBALINTFLAG = ''+'' ';
v_sql := v_sql||'                                        THEN ';
v_sql := v_sql||'                                           S.STM_BALINT ';
v_sql := v_sql||'                                        ELSE ';
