v_sql := v_sql||'                       * COAL (B.RATE_VAL0, 0) ';
v_sql := v_sql||'                     +   COAL (T2.BAL_FREE, 0) ';
v_sql := v_sql||'                       * COAL (B.RATE_VAL0, 0) ';
v_sql := v_sql||'                     + CASE ';
