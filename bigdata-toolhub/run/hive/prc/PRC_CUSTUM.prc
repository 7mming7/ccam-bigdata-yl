v_sql := v_sql||'                                   S.CRED_LIMIT CRED_LIMIT, ';
v_sql := v_sql||'                                   A.MP_L_LMT MP_L_LMT, ';
v_sql := v_sql||'                                   date_diff(  S.END_DATE ';
