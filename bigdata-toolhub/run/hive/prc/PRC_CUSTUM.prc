v_sql := v_sql||'                                                      toDate(S.TLMT_BEG, ';
v_sql := v_sql||'                                                               ''yyyymmdd'') ';
v_sql := v_sql||'                                              THEN ';
v_sql := v_sql||'                                                 toDate(S.TLMT_BEG, ';
v_sql := v_sql||'                                                          ''yyyymmdd'') ';
v_sql := v_sql||'                                              ELSE ';
v_sql := v_sql||'                                                 CASE ';
