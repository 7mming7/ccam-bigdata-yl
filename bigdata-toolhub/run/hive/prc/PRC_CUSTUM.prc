v_sql := v_sql||'                          THEN ';
v_sql := v_sql||'                               COAL (T2.BAL_INT, 0) ';
v_sql := v_sql||'                             * COAL (B.RATE_VAL0, 0) ';
v_sql := v_sql||'                          ELSE ';
v_sql := v_sql||'                             0 ';
v_sql := v_sql||'                       END ';
v_sql := v_sql||'                     +   COAL (T2.BAL_MP, 0) ';
v_sql := v_sql||'                       * COAL (B.RATE_VAL0, 0) ';
v_sql := v_sql||'                     +   COAL (T2.BAL_ORINT, 0) ';
v_sql := v_sql||'                       * COAL (B.RATE_VAL0, 0) ';
v_sql := v_sql||'                     +   COAL (T2.BAL_NOINT, 0) ';
