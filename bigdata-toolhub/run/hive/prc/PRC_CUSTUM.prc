v_sql := v_sql||'                                                          ) src ';
v_sql := v_sql||'                  GROUP BY SRC.XACCOUNT, SRC.CUSTR_NBR) fa ';
v_sql := v_sql||'        GROUP BY fa.CUSTR_NBR) acct left join ';
v_sql := v_sql||'       (  SELECT T.CUSTR_NBR, ';
v_sql := v_sql||'                 SUM ( ';
v_sql := v_sql||'                      (    COAL (T2.STM_BALFRE, 0) ';
