v_sql := v_sql||'                                                                          ADD_MONTHS ( ';
v_sql := v_sql||'                                                                             SYSDATE(), ';
v_sql := v_sql||'                                                                             -' || months || ') ';
v_sql := v_sql||'                                                                    END ';
v_sql := v_sql||'                                                          END ';
v_sql := v_sql||'                                          ELSE ';
v_sql := v_sql||'                                           0 ';
