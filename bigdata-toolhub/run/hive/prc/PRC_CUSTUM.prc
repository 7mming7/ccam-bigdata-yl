v_sql := v_sql||'                 AND todate(T2.END_DATE,''yyyy-mm-dd'') < date_add(sysdate(),1) ';
v_sql := v_sql||'                 AND tc.bank_id is null ';
v_sql := v_sql||'        GROUP BY T.CUSTR_NBR) acca on ACCT.CUSTR_NBR = acca.CUSTR_NBR ';

