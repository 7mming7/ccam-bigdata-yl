v_sql := v_sql||'                         * COAL (B.RATE_VAL0, 0) ';
v_sql := v_sql||'                                                    ) ';
v_sql := v_sql||'                    * (  T2.END_DATE ';
v_sql := v_sql||'                       - CASE ';
